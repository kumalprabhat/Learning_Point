<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.learningpoint">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".Sql_Result"></activity>
        <activity android:name=".Python_Result" />
        <activity android:name=".Java_Result" />
        <activity android:name=".Cpp_Result" />
        <activity android:name=".Sql_Quiz" />
        <activity android:name=".Python_Quiz" />
        <activity android:name=".Java_Quiz" />
        <activity android:name=".Cpp_Quiz" />
        <activity android:name=".Result" />
        <activity android:name=".C_Quiz" />
        <activity android:name=".Quiz" />
        <activity android:name=".Description" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>